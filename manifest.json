{
  "name": "Console Preview & Log Notifications",
  "version": "0.1.0",
  "manifest_version": 2,
  "offline_enabled": true,
  "description": "show console logs and alerts as chrome notifications",
  "homepage_url": "https://chrome.google.com/webstore/detail/log-notifications/dkebpeehodefnopcpggkdgeljadommmm?utm_source=plus",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Console Output",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "notifications","tabs", "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/inject/inject.js"
  ]
}