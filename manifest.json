{
  "name": "Log Notifications",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "show console logs and alerts as chrome notifications",
  "homepage_url": "http://about.me/qusai87",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/inject/inject.js"
  ]
}